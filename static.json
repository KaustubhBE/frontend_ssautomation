{
  "root": "dist",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Content-Type": "application/javascript",
      "X-Frame-Options": "DENY"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000",
      "Content-Type": "application/javascript"
    }
  }
} 